{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AzureKeyVault": {
    "Name": "{AZURE-APPSVC-ENV}",
    "SmtpPasswordKey": "{AZURE-APPSVC-ENV}"
  },
  "AppSettings": {
    "EnableEmailSending": true,
    "EnableSsl": true,
    "SmtpServerPort": 587,
    "AdminEmail": "{AZURE-APPSVC-ENV}",
    "EmailDisplayName": "{AZURE-APPSVC-ENV}",
    "SmtpServer": "{AZURE-APPSVC-ENV}",
    "SmtpUserName": "{AZURE-APPSVC-ENV}",
    "ApiKeys": [
      {
        "Id": 1,
        "Owner": "https-edi.wang",
        "Key": "{AZURE-APPSVC-ENV}"
      }
    ]
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*:/api/*",
        "Period": "1s",
        "Limit": 3
      },
      {
        "Endpoint": "*:/api/*",
        "Period": "1m",
        "Limit": 15
      }
    ]
  },
  "AllowedHosts": "*"
}
